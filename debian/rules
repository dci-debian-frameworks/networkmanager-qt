#!/usr/bin/make -f
# -*- makefile -*-

%:
	dh $@ --with kf5,pkgkde_symbolshelper --buildsystem kf5

override_dh_auto_configure:
	dh_auto_configure -- -DBUILD_QCH=ON

override_dh_strip:
	dh_strip --dbgsym-migration='libkf5networkmanagerqt6-dbg (<= 5.19.0-1~~)'

override_dh_auto_test:
	# Disable tests at build time
	:
